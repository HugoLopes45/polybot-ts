	"files": ["dist"],
	"scripts": {
		"build": "tsup",
		"dev": "tsup --watch",
		"typecheck": "tsc --noEmit",
		"lint": "biome check src/",
		"lint:fix": "biome check --write src/",
		"test": "vitest run",
		"test:watch": "vitest",
		"test:coverage": "vitest run --coverage",
		"test:integration": "vitest run --project integration",
		"ci": "pnpm lint && pnpm typecheck && pnpm test",
		"prepublishOnly": "pnpm ci",
		"docs:typedoc": "typedoc",
		"docs:copy-api": "shx rm -rf docs/api && shx mkdir -p docs/api && shx cp -r .docs-api/* docs/api/ && shx rm -rf .docs-api && shx rm -rf docs/api/_media",
		"docs:gen-index": "node scripts/gen-api-index.mjs",
		"docs:dev": "pnpm docs:typedoc && pnpm docs:copy-api && pnpm docs:gen-index && vitepress dev docs",
		"docs:build": "pnpm docs:typedoc && pnpm docs:copy-api && pnpm docs:gen-index && vitepress build docs",
		"docs:preview": "vitepress preview docs"
	},
	"devDependencies": {
		"@biomejs/biome": "^1.9.4",
